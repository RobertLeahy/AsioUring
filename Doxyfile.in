PROJECT_NAME="ASIO Uring"
PROJECT_BRIEF="Integration of io_uring with Boost.Asio"
OUTPUT_DIRECTORY="doc"
INPUT=${CMAKE_SOURCE_DIR}/src/asio \
      ${CMAKE_SOURCE_DIR}/src/core
EXCLUDE_PATTERNS=
FILE_PATTERNS=*.hpp
RECURSIVE=YES
INPUT_FILTER=
FILTER_SOURCE_FILES=NO
STRIP_FROM_INC_PATH=${CMAKE_SOURCE_DIR}/src/asio/include \
                    ${CMAKE_SOURCE_DIR}/src/core/include
PREDEFINED=ASIO_URING_DOXYGEN_RUNNING
EXCLUDE_SYMBOLS=detail
DISTRIBUTE_GROUP_DOC=YES
EXTRACT_ALL=YES
${DOXYGEN_CONFIGURE_HAVE_DOT}
${DOXYGEN_CONFIGURE_DOT_PATH}
